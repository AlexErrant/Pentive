{
	"extends": "../../tsconfig.options.json",
	"compilerOptions": {
		"emitDeclarationOnly": false,
		"noEmit": true,
		"strict": true,
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"types": ["vite/client"]
	},
	"include": ["src/**/*", "*.ts"]
}
