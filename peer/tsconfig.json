{
	"extends": "config/tsconfig.json",
	"compilerOptions": {
		"emitDeclarationOnly": false,
		"noEmit": true,
		"strict": true,
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"types": [
			"node"
		]
	},
	"ts-node": {
		"esm": true
	},
	"include": [
		"server.ts"
	]
}