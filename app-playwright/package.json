{
	"name": "app-playwright",
	"version": "0.0.0",
	"type": "module",
	"sideEffects": false,
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"tsc-build": "tsc",
		"eslint": "eslint . --max-warnings 0",
		"eslint:fix": "eslint . --fix --max-warnings 0",
		"format:check": "prettier --ignore-path .gitignore --check .",
		"format": "prettier --ignore-path .gitignore --write .",
		"test:run": "playwright test",
		"test:dev": "MODE=development playwright test --grep-invert @prod",
		"stylelint": "stylelint **/*.css",
		"stylelint:write": "stylelint **/*.css --fix",
		"pack": "pnpm pack",
		"playwright:init": "playwright install"
	},
	"devDependencies": {
		"@playwright/test": "1.49.0-alpha-2024-11-09",
		"@solidjs/router": "^0.15.3",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "20.14.11",
		"config": "workspace:*",
		"dotenv": "^16.3.1",
		"typescript": "~5.7.3",
		"vite": "^6.2.0",
		"vite-plugin-solid": "^2.11.6"
	},
	"dependencies": {
		"@scure/base": "^1.2.4",
		"app": "workspace:*",
		"fast-check": "^3.23.1",
		"lodash-es": "^4.17.21",
		"shared": "workspace:*",
		"solid-js": "^1.9.5"
	}
}
