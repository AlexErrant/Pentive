{
  "name": "rxql",
  "version": "0.0.0",
  "scripts": {
    "dev": "webpack serve",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "server": "ts-node-dev ./server/index.ts",
    "refresh": "rimraf -r node_modules/rxdb/ && npm i ",
    "build": "webpack",
    "build:serve": "ws -p 8888 -d dist/",
    "test:e2e": "testcafe chrome -e test/ --hostname localhost",
    "test:pouchdb": "STORAGE=pouchdb concurrently \"npm run server\" \"npm run build:serve\" \"sleep 10 && npm run test:e2e\" --kill-others --success first",
    "test:lokijs": "STORAGE=lokijs concurrently \"npm run server\" \"npm run build:serve\" \"sleep 10 && npm run test:e2e\" --kill-others --success first",
    "test:dexie": "STORAGE=dexie concurrently \"npm run server\" \"npm run build:serve\" \"sleep 10 && npm run test:e2e\" --kill-others --success first",
    "eslint": "eslint .",
    "eslint:write": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "browser": {
    "fs": false
  },
  "private": true,
  "author": "pubkey",
  "license": "MIT",
  "dependencies": {
    "concurrently": "7.3.0",
    "cors": "2.8.5",
    "express": "^4.18.1",
    "express-graphql": "0.12.0",
    "graphql": "^15.3.0",
    "graphql-client": "2.0.1",
    "lodash": "^4.17.21",
    "normalize.css": "8.0.1",
    "pouchdb-adapter-idb": "7.2.2",
    "process": "^0.11.10",
    "rxdb": "13.4.4",
    "rxjs": "7.5.6",
    "ws": "8.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.7.21",
    "async-test-util": "2.0.0",
    "css-loader": "6.7.1",
    "eslint": "7.32.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "ncp": "2.0.0",
    "rimraf": "3.0.2",
    "source-map-loader": "3.0.1",
    "style-loader": "3.3.1",
    "testcafe": "1.20.1",
    "testcafe-hammerhead": "24.7.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.7.4"
  }
}