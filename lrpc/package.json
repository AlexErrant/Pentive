    "@prisma/client": "^4.6.1",
    "@trpc/client": "^10.11.0",
    "@trpc/server": "^10.11.0",
    "@vlcn.io/crsqlite-allinone": "^0.6.0",
    "aws-lambda": "^1.0.7",
    "cookie": "^0.5.0",
    "dotenv": "^16.0.2",
    "html-to-text": "^8.2.1",
    "id128": "^1.6.6",
    "jose": "^4.11.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "shared": "workspace:*",
    "superjson": "^1.12.2",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.106",
    "@types/cookie": "^0.5.1",
    "@types/html-to-text": "^8.1.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.7.21",
    "@types/node-fetch": "^2.6.2",
    "@types/uuid": "^9.0.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.6.1",
    "serverless": "^3.22.0",
    "serverless-offline": "^10.2.1",
    "serverless-plugin-typescript": "^2.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "npx tsc",
    "dev": "nodemon src/dev.ts",
    "//dev-comment": "maybe someday https://typestrong.org/ts-node/docs/swc/ https://github.com/privatenumber/ts-runtime-comparison abandoned ts-node-dev because of https://github.com/wclr/ts-node-dev/issues/265",
    "start-offline": "npx serverless offline start --httpPort 4050",
    "//generate-cert": "requires installing https://github.com/FiloSottile/mkcert ",
    "generate-cert": "mkdir .cert && mkcert -key-file ./.cert/key.pem -cert-file ./.cert/cert.pem lrpc.local.pentive.com",
    "rebuild-offline": "npm run build && npm run start-offline",
    "start-client": "yarn ts-node src/client.ts",
    "http-rest-client": "yarn ts-node src/payloadFormatVersionClient.ts",
    "eslint": "eslint .",
    "eslint:write": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:write": "prettier --write ."
  }
}